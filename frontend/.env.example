# Example environment variables - Copy this file to .env and update with your own values

# Application settings
VITE_APP_TITLE=Balance Scale Addition Game

# API Configuration
VITE_API_URL=http://localhost:8000

# Firebase Configuration - Replace with your Firebase project details
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Development settings
VITE_USE_EMULATORS=true  # Set to false for production or when using real Firebase services 